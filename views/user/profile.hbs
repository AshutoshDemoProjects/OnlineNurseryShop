<div class="row">
    <div class='col-md-3'>
        <div class="form-group">
            <div class='input-group' id='datetimepicker6'>
                <input type='date' class="form-control" />
                <div class="input-group-prepend">
                    <div class="input-group-text"><i class="far fa-calendar-alt"></i></div>
                </div>
            </div>
        </div>
    </div>
    <div class='col-md-3'>
        <div class="form-group">
            <div class='input-group'>
                <input type='date' class="form-control" id='datetimepicker7' />

                <div class="input-group-prepend">
                    <div class="input-group-text"><i class="far fa-calendar-alt"></i></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-2">
        <button class="btn" type="submit" id="button-addon2"><i class="fas fa-search"></i></button>
    </div>
</div>

<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <h1>User Profile</h1>
        <hr>
        <h2>My Orders</h2>
        {{# each orders }}
        <div class="panel panel-default">
            <div class="panel-body">
                <ul class="list-group">
                    {{# each this.items }}
                    <li class="list-group-item">
                        <span class="badge">{{ this.price }}</span>
                        {{ this.item.title }} | {{ this.qty }} Units
                    </li>
                    {{/each}}
                </ul>
            </div>
            <div class="panel-footer">
                <strong>Total Price: ${{ this.cart.totalPrice }}</strong>
            </div>
        </div>
        {{/each}}
    </div>
</div>